    </head>

    <body>
        <h2>COURSE LIST</h2>
        <form  class = "form-horizontal" action = "/view/NoteService/sendTopicView" method = "post"> 
        <table width="600" border="1" cellspacing="1" cellpadding="1">
            <tr bgcolor="#999999">
                <td>Course ID</td>
            </tr>
         <% for(  note in topic_list) { %>
            <tr>
                <td>
                    ${note.topic}
                </td>
            </tr>
        <% }  %>
        </table>
      <div class="form-group">
		<div class="col-lg-2">
		<button type="submit" class="btn btn-primary" id="sendTopicView" >Login</button>
		</div>
      </div>
      </form>
    </body>
</html