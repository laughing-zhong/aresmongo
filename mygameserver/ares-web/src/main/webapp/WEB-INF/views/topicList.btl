<html>
    <head>
        <title>Course List</title>
    </head>

    <body>
        <h2>COURSE LIST</h2>
        <table width="600" border="1" cellspacing="1" cellpadding="1">
            <tr bgcolor="#999999">
                <td>Course ID</td>
            </tr>
        <% if(has(topic_list)) { 
          for(  note in topic_list) { %>
            <tr>
                <td>
                    ${note.topic}
                </td>
            </tr>
        <% }
        } %>
        </table>
    </body>
</html